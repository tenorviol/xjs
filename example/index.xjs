<html>
<head>
  <title>Index of {{= __dirname }}</title>
</head>
<body>
  <h1>Index of {{= __dirname }}</h1>
  <ul>
  {{
    var async = response.asyncStream();
    var files = require('fs').readdir(__dirname, function(err, files) {
      files.forEach(function(file) {
        async.write('<li>' + file + '</li>');
      });
      async.end();
    });
  }}
  </ul>
</body>
</html>
